{
  "parser": "babel-eslint",
  "rules": {
    "strict": 2,
    "no-console": 0,
    "quotes": [2, "double"],
    "semi": [2, "never"],
    "object-curly-spacing": [2, "never"],
    "space-before-function-paren": [2, "never"]
  },
  "env": {
    "node": true
  },
  "extends": "eslint:recommended"
}
